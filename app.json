{
  "expo": {
    "name": "Green Streak",
    "slug": "green-streak",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.green-streak.app",
      "appleTeamId": "974SWJKYUP",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "com.apple.security.application-groups": [
          "group.com.greenstreak.shared"
        ]
      },
      "buildNumber": "6"
    },
    "scheme": "greenstreak",
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-sqlite",
      "@bacons/apple-targets",
      "./plugins/withWidgetExtension"
    ],
    "extra": {
      "eas": {
        "projectId": "8a77c913-c0fc-4d68-b875-067cc3ea8cca",
        "build": {
          "experimental": {
            "ios": {
              "appExtensions": [
                {
                  "targetName": "GreenStreakWidgets",
                  "bundleIdentifier": "com.green-streak.app.GreenStreakWidgets",
                  "entitlements": {
                    "com.apple.security.application-groups": [
                      "group.com.greenstreak.shared"
                    ]
                  }
                }
              ]
            }
          }
        }
      }
    }
  }
}
